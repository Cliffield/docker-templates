<?xml version="1.0" encoding="utf-8"?>
<Container>
  <TemplateURL>https://raw.githubusercontent.com/cliffield/docker-templates/master/cliffield/docker-minimserver.xml</TemplateURL>
<Beta>False</Beta>
<Category>Downloaders: MediaApp:DLNA</Category>
<Date>2016-08-08</Date>
 <Name>MinimServer</Name>
  <Changes>
  </Changes>
  <Description>A
  </Description>
  <Project>https://github.com/Cliffield/MinimServer</Project>
  <Overview></Overview>
  <Registry>https://hub.docker.com/r/cliffield/docker-minimserver/</Registry>
  <Repository>cliffield/docker-minimserver</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>1900</HostPort>
        <ContainerPort>1900</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
      <Port>
        <HostPort>9790</HostPort>
        <ContainerPort>9790</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>9791</HostPort>
        <ContainerPort>9791</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/Media/Musik</HostDir>
      <ContainerDir>/music</ContainerDir>
      <Mode>r</Mode>
    </Volume>
  </Data>
  <Icon></Icon>
  <WebUI></WebUI>
<BaseImage>lsiobase/alpine:3.9</BaseImage>
</Container>

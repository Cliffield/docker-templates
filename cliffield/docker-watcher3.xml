<?xml version="1.0" encoding="utf-8"?>
<Container>
  <TemplateURL>https://raw.githubusercontent.com/cliffield/docker-templates/master/cliffield/docker-watcher3.xml</TemplateURL>
<Beta>False</Beta>
<Category>Downloaders: MediaApp:Video</Category>
<Date>2016-08-08</Date>
 <Name>watcher3</Name>
  <Changes>
  </Changes>
  <Description>Automatic Video Library Manager for Movies.
  </Description>
  <Project>https://github.com/Cliffield/watcher3</Project>
  <Overview>Automatic Video Library Manager for Movies.</Overview>
  <Registry>https://hub.docker.com/r/cliffield/docker-watcher3/</Registry>
  <Repository>cliffield/docker-watcher3</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>9090</HostPort>
        <ContainerPort>9090</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/docker-watcher3</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/downloads</HostDir>
      <ContainerDir>/downloads</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/Media/Filme</HostDir>
      <ContainerDir>/Filme</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Icon></Icon>
  <WebUI>http://[IP]:[PORT:9090]/</WebUI>
<BaseImage>lsiobase/alpine.python3:3.6</BaseImage>
</Container>